	xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="controller.ServerController">
	<children>
		<TextArea fx:id="serverView" layoutX="28.0" layoutY="20.0"
			prefHeight="174.0" prefWidth="365.0" />
		<Button fx:id="btnStart" layoutX="141.0" layoutY="204.0"
			mnemonicParsing="false" text="Start" onAction="#startAction" />
		<Button fx:id="btnStop" layoutX="211.0" layoutY="204.0"
			mnemonicParsing="false" text="Stop" onAction="#stopAction" />
	</children>
</AnchorPane>